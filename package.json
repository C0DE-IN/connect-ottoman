{
  "name": "connect-ottoman",
  "version": "1.0.1-beta",
  "description": "connect-ottoman provides Couchbase Ottoman session storage for Express. Requires Ottoman >= 2.0.0-beta.2",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only src/example",
    "prebuild": "rm -rf dist",
    "build": "tsc && cp package.json README.md dist/",
    "postbuild": "perl -0777 -pi -e 's/\"scripts\".*\"repository\"/\"repository\"/igs' dist/package.json",
    "publish": " cd dist && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noiissyboy/connect-ottoman.git"
  },
  "keywords": [
    "couchbase",
    "ottoman",
    "express-session",
    "store",
    "session-store"
  ],
  "author": "Vigneshwaran Ranganathan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/noiissyboy/connect-ottoman/issues"
  },
  "homepage": "https://github.com/noiissyboy/connect-ottoman#readme",
  "devDependencies": {
    "@types/express-session": "^1.17.3",
    "express-session": "^1.17.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "ottoman": "^2.0.0-beta.2"
  }
}